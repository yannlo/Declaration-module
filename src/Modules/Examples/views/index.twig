{% extends "layout.twig" %}

{% block title "Articles" %}


{% block content %}
<h1>Liste des articles</h1>
<p>
    <ul>
        {% for article in articles %}
            <li><a href="{{ article.slug()}}-{{article.id() }}">{{ article.title() }}</a></li>
        {% endfor %}
    </ul>
</p>
{% endblock %}